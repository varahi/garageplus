lib.logo >
lib.logo = COA
lib.logo {
    wrap = <div class="col-xs-12 col-md-4">|</div>
    10 = IMAGE
    10 {
        file = EXT:tmpl/Resources/Public/img/logo.png
        altText = {$config.header.logoTitle}
        titleText = {$config.header.logoTitle}
        file.width = 337
        file.height = 32
        stdWrap.typolink.parameter =  {$config.navigation.rootPid}
    }
    20 = TEXT
    20 {
        wrap = <div class="slogan">|</div>
        value = {$config.header.slogan}
    }
}

